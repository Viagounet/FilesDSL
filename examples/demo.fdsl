# Basic traversal
docs = Directory("../data")
pdf_files = docs.search(".*\\.pdf$", scope="name")
print("PDF files:", len(pdf_files))
for file in pdf_files:
    if file.contains("language model", ignore_case=true):
        print("match:", file)
        first = file.head()
        print("head length:", len(first))
        print(file.table())
        print("===========")
# Range syntax in lists (inclusive)
some_pages = [1, 3:5, 8]
print("expanded pages:", some_pages)
